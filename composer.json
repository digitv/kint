{
    "name": "digitv/kint",
    "version": "1.0.1",
    "description": "Add kint support",
    "keywords": ["yii2", "kint", "debug"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/digitv/kint/issues",
        "source": "https://github.com/digitv/kint"
    },
    "authors": [
        {
            "name": "Digit",
            "email": "digit.vova@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "digitv\\kint\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}